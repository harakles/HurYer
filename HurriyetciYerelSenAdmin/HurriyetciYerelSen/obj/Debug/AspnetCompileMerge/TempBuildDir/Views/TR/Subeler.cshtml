@using HurriyetciYerelSen.EDMX
@model List<Branch>
<!--Start breadcrumb area-->
<section class="breadcrumb-area" style="background-image: url();">
    <div class="container">
        <div class="row">
            <div class="col-xl-12">
                <div class="inner-content text-center clearfix">
                    <div class="title wow slideInUp animated" data-wow-delay="0.3s" data-wow-duration="1500ms">
                        <h2>Şubeler</h2>
                    </div>
                    <div class="breadcrumb-menu wow slideInDown animated" data-wow-delay="0.3s" data-wow-duration="1500ms">
                        <ul class="clearfix">
                            <li><a href="/TR/AnaSayfa">AnaSayfa</a></li>
                            <li><i class="fa fa-angle-double-right" aria-hidden="true"></i></li>
                            <li class="active">Şubeler</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="coaching-style2-area">
    <div class="container">
        <div class="row">
            @foreach (var x in Model)
            {
                <div class="col-xl-4 col-lg-6 wow fadeInUp" data-wow-delay="0ms">
                    <a href="/TR/SubeDetay?Id=@x.Id">
                        <div class="single-coaching-item-style2 text-center">
                            <div class="coaching-item-style2-bg" style="background-image: url()"></div>
                            <div class="inner">
                                <div class="icon"><span class="flaticon-architecture-and-city"></span></div>
                                <h3>@x.BranchName</h3>
                                <p>@x.BranchNumber</p>
                            </div>
                        </div>
                    </a>
                </div>
            }
        </div>
    </div>
</section>
